      \strng{fullhash}{821e2a6be30aa2d7bac6f9b37297d593}
      \strng{fullhashraw}{821e2a6be30aa2d7bac6f9b37297d593}
      \strng{bibnamehash}{821e2a6be30aa2d7bac6f9b37297d593}
      \strng{authorbibnamehash}{821e2a6be30aa2d7bac6f9b37297d593}
      \strng{authornamehash}{167153cd956f96f7efebb99b2cc091e1}
