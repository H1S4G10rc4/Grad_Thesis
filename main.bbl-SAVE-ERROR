      \strng{namehash}{53469751151b308609e91ff62566086f}
      \strng{fullhash}{07876d77c181be2fdcf2cf6b7c517506}
      \strng{fullhashraw}{07876d77c181be2fdcf2cf6b7c517506}
      \strng{bibnamehash}{07876d77c181be2fdcf2cf6b7c517506}
      \strng{authorbibnamehash}{07876d77c181be2fdcf2cf6b7c517506}
      \strng{authornamehash}{53469751151b308609e91ff62566086f}
