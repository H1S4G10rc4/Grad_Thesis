    \entry{Lucena2019}{article}{}{}
      \name{author}{5}{}{%
        {{un=0,uniquepart=base,hash=35061d2992fa5c2dbb842b364a7c815a}{%
           family={Lucena},
           familyi={L\bibinitperiod},
           given={Oeslle},
