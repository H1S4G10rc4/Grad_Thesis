      \strng{namehash}{8dbba6cf76945635c02deca58d3388b7}
      \strng{fullhash}{df524d1c7d49cda3ec747b2ac38e6c04}
      \strng{fullhashraw}{df524d1c7d49cda3ec747b2ac38e6c04}
      \strng{bibnamehash}{df524d1c7d49cda3ec747b2ac38e6c04}
      \strng{authorbibnamehash}{df524d1c7d49cda3ec747b2ac38e6c04}
