    \entry{Warfield2004}{article}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=54d88f825ed9dee7ef04bcb244ae0920}{%
           family={Warfield},
           familyi={W\bibinitperiod},
           given={Simon\bibnamedelima K.},
           giveni={S\bibinitperiod\bibinitdelim K\bibinitperiod},
           givenun=0}}%
        {{un=0,uniquepart=base,hash=a8bcbc990bdd3345ff5e3090e5dae80d}{%
